{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": [
      "plugin:react/recommended",
      "standard-with-typescript"
  ],
  "overrides": [
  ],
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": [
      "react"
  ],
  "rules": {
      "indent": "off",
      "comma-dangle": "off",
      "require-jsdoc":"off",
      "semi": "off",
      "space-before-function-paren": "off"
  }
}
